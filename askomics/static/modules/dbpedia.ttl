@base <http://www.semanticweb.org/irisa/ontologies/2016/1/igepp-ontology#> .
<http://www.semanticweb.org/irisa/ontologies/2016/1/igepp-ontology#> rdf:type owl:Ontology .

[] a sd:Service ;
    sd:endpoint <https://dbpedia.org/sparql> ;
    dcterms:title "DBPedia";
    dcterms:description "http://wiki.dbpedia.org/";
    sd:supportedLanguage sd:SPARQL11Query ;
    sd:defaultDataset [
        a sd:Dataset ;
        sd:defaultGraph [
            a sd:Graph ;
            void:classPartition [ void:class dbpedia:Album. ].
        ].
    ].


dbpedia:Album displaySetting:entity "true"^^xsd:boolean ;
              displaySetting:remoteEntity "true"^^xsd:boolean ;
              displaySetting:startPoint "true"^^xsd:boolean ;
              rdfs:label "dbpedia:Album".

dbpedia:albumRuntime displaySetting:attribute "true"^^xsd:boolean ;
                                         rdf:type owl:DatatypeProperty ;
                                         rdfs:label "album duration" ;
                                         rdfs:domain dbpedia:Album ;
                                         rdfs:range xsd:double .

dbpedia:longtype displaySetting:attribute "true"^^xsd:boolean ;
                                         rdf:type owl:DatatypeProperty ;
                                         rdfs:label "longtype" ;
                                         rdfs:domain dbpedia:Album ;
                                         rdfs:range xsd:string .

dbpedia:musicFormat displaySetting:attribute "true"^^xsd:boolean ;
                                         rdf:type owl:DatatypeProperty ;
                                         rdfs:label "musicFormat" ;
                                         rdfs:domain dbpedia:Album ;
                                         rdfs:range xsd:string .

dbpedia:review displaySetting:attribute "true"^^xsd:boolean ;
                                         rdf:type owl:DatatypeProperty ;
                                         rdfs:label "review" ;
                                         rdfs:domain dbpedia:Album ;
                                         rdfs:range xsd:string .

dbpedia:totalDiscs displaySetting:attribute "true"^^xsd:boolean ;
                                         rdf:type owl:DatatypeProperty ;
                                         rdfs:label "totalDiscs" ;
                                         rdfs:domain dbpedia:Album ;
                                         rdfs:range xsd:integer .

dbpedia:totalTracks displaySetting:attribute "true"^^xsd:boolean ;
                                         rdf:type owl:DatatypeProperty ;
                                         rdfs:label "totalTracks" ;
                                         rdfs:domain dbpedia:Album ;
                                         rdfs:range xsd:integer .
