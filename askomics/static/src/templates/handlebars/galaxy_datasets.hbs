<p>Select a galaxy history: </p>
<select id="change_history">
  {{#each histories}}
    {{#if this.selected}}
        <option value="{{this.id}}" selected="selected">{{this.name}}</option>
    {{else}}
        <option value="{{this.id}}">{{this.name}}</option>
    {{/if}}
  {{/each}}
</select>
<br/ ><br/ >

<i class='fa fa-2x fa-spinner pulse hidden' id="spinner_loading_galaxy"></i>
<br />
<table class='table data-table galaxy-table'>
    <thead>
        <tr>
            <th>
            {{#if radio}}
                <input class="check_galaxy check_all_galaxy" type="checkbox">
            {{/if}}
            </th>
            <th>Id</th>
            <th>File</th>
            <th>Type</th>
        </tr>
    </thead>
    <tbody>
        {{#each datasets}}
        <tr>
            <td><input class="check_galaxy check_one_galaxy" type="{{../input_type}}" name="upload-galaxy" value="{{this.id}}" {{#if this.notick}} class='disabled'{{/if}}></td>
            {{#if this.success}}
            <td><i class="text-success">{{this.hid}}</i></td>
            <td><i class="text-success">{{this.name}}</i></td>
            <td><i class="text-success">{{this.extension}}</i></td>
            {{/if}}
            {{#if this.queued}}
            <td><i class="text-warning">{{this.hid}}</i></td>
            <td><i class="text-warning">{{this.name}}</i></td>
            <td><i class="text-warning">{{this.extension}}</i></td>
            {{/if}}
            {{#if this.error}}
            <td><i class="text-danger">{{this.hid}}</i></td>
            <td><i class="text-danger">{{this.name}}</i></td>
            <td><i class="text-danger">{{this.extension}}</i></td>
            {{/if}}
        </tr>
        {{/each}}
    </tbody>
</table>

<button id="upload_galaxy" type="button" class="btn btn-default" disabled><i class="fa fa-cloud-upload text-primary"></i> Upload to AskOmics</button>
<i class="hidden fa fa-spinner pulse" id='spinner_galaxy-upload'></i>